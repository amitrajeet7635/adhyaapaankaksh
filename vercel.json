{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "outputDirectory": "dist",
    "cleanUrls": true,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  